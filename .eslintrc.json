{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["vuetify"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "allowIndentationTabs": true,
    "max-len": [
      "error",
      {
        "code": 120
      }
    ]
  }
}
